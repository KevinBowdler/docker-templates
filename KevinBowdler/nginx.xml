<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>nginx_[APPNAME]</Name>
  <Description>NGINX template [br]IMPORTANT :- YOU MUST CHANGE THE NAME, HOST PORT AND VOLUMES BELOW</Description>
  <Registry>https://hub.docker.com/_/nginx/</Registry>
  <Repository>nginx</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8087</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/nginx_[APPNAME]/nginx.conf</HostDir>
      <ContainerDir>/etc/nginx/nginx.conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/nginx_[APPNAME]/html</HostDir>
      <ContainerDir>/usr/share/nginx/html</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[iP]:[PORT:81]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Container>
