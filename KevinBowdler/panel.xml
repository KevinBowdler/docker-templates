<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Panel</Name>
  <Description>Panel template IMPORTANT :- YOU MUST CHANGE THE NAME, VOLUME AND PORT BELOW</Description>
  <Registry>https://hub.docker.com/r/kevinb/texecom-slack-bot</Registry>
  <Repository>kevinb/texecom-slack-bot</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>10011</HostPort>
        <ContainerPort>10011</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
    <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/panel/config</HostDir>
      <ContainerDir>/usr/panel/app/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
    <Environment>
    <Variable>
      <Name>HOST</Name>
      <Value>0.0.0.0</Value>
    </Variable>
    <Variable>
      <Name>PORT</Name>
      <Value>10011</Value>
    </Variable>
  </Environment>
  <Version></Version>
  <WebUI>http://[iP]:[PORT:81]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Container>