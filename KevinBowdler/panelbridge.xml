<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>panel-bridge</Name>
  <Description>panel-bridge template IMPORTANT :- YOU MUST CHANGE THE NAME, VOLUME AND PORT BELOW</Description>
  <Registry>https://hub.docker.com/r/kevinb/panel-bridge</Registry>
  <Repository>kevinb/panel-bridge</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>10011</HostPort>
        <ContainerPort>10011</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/panel-bridge/logs</HostDir>
      <ContainerDir>/usr/app/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
    <Environment>
    <Variable>
      <Name>HOST</Name>
      <Value>0.0.0.0</Value>
    </Variable>
    <Variable>
      <Name>PORT</Name>
      <Value>10011</Value>
    </Variable>
    <Variable>
      <Name>SLACK_WEBHOOK</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>SLACK_CHANNEL</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>SLACK_USER</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>LOG_LEVEL</Name>
      <Value>info</Value>
    </Variable>
    <Variable>
      <Name>LOG_ROOT</Name>
      <Value>./logs</Value>
    </Variable>
    <Variable>
      <Name>LOG_FILECOUNT</Name>
      <Value>10</Value>
    </Variable>
  </Environment>
  <Version></Version>
  <WebUI>http://[iP]:[PORT:81]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Container>