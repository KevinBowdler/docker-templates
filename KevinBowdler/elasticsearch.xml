<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>ElasticSearch</Name>
  <Description>Official ElasticSearch Image</Description>
  <Registry>docker.elastic.co/elasticsearch/elasticsearch</Registry>
  <Repository>elasticsearch</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>9200</HostPort>
        <ContainerPort>9200</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>9300</HostPort>
        <ContainerPort>9300</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/elasticsearch/esdata</HostDir>
      <ContainerDir>/usr/share/elasticsearch/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[iP]:9200/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Container>