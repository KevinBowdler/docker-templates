<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Wordpress</Name>
  <Description>Wordpress template for use with MySql[br]IMPORTANT :- YOU MUST CHANGE THE HOST PORT BELOW</Description>
  <Registry>https://hub.docker.com/_/wordpress/</Registry>
  <Repository>wordpress</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>81</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/wordpress_test/www/html</HostDir>
      <ContainerDir>/var/www/html</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/wordpress_test/www/html/wp-content</HostDir>
      <ContainerDir>/var/www/html/wordpress/wp-content</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[iP]:[PORT:81]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams>--link mysql:mysql -e WORDPRESS_DB_NAME=wordpress_test</ExtraParams>
</Container>